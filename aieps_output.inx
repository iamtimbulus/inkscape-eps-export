<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <name>AI compatible EPS output</name>
    <id>org.inkscape.output.ai_eps</id>
    <dependency type="executable" location="extensions">aieps_output.py</dependency>
    <output>
        <extension>.eps</extension>
        <mimetype>application/eps</mimetype>
        <filetypename>Encapsulated PostScript - AI compatible (*.eps)</filetypename>
        <filetypetooltip>Adobe Illustrator 7 compatible EPS</filetypetooltip>
    </output>
    <script>
        <command reldir="extensions" interpreter="python">aieps_output.py</command>
    </script>
</inkscape-extension>
